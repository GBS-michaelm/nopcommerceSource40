@model ShoppingCartModel
@using Nop.Web.Models.ShoppingCart;
@if (Model.ButtonPaymentMethodActionNames != null && Model.ButtonPaymentMethodActionNames.Count > 0)
{
    for (int i = 0; i < Model.ButtonPaymentMethodActionNames.Count; i++)
    {
    @await Component.InvokeAsync(Model.ButtonPaymentMethodActionNames[i], Model.ButtonPaymentMethodControllerNames[i], Model.ButtonPaymentMethodRouteValues[i])
    }
}
