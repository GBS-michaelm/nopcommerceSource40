@using Nop.Web.Models.ShoppingCart;
@using System.Web.UI;



<div class="card-image-section info-icons">
    <div class="title">
        <strong>@T("shoppingcart.cardimage.title")</strong>
    </div>
    <div class="card-type-image-section">
        <i class="fa fa-cc-visa fa-3x gbs-visa-icon-color" aria-hidden="true"></i>
        <i class="fa fa-cc-mastercard fa-3x gbs-master-card-icon-color" aria-hidden="true"></i>
        <i class="fa fa-cc-amex fa-3x gbs-amex-card-icon-color" aria-hidden="true"></i>
        <i class="fa fa-cc-discover fa-3x gbs-discover-card-icon-color" aria-hidden="true"></i>
    </div>
    <div class="card-types-security-image-section">
        <table width="135" border="0" cellpadding="2" cellspacing="0" title="Click to Verify - This site chose Symantec SSL for secure e-commerce and confidential communications.">
            <tr>
                <td width="135" align="center" valign="top">
                    <script type="text/javascript" src="https://seal.websecurity.norton.com/getseal?host_name=http://www.houseofmagnets.com&amp;size=L&amp;use_flash=NO&amp;use_transparent=NO&amp;lang=en"></script><br />
                    <a href="http://www.symantec.com/ssl-certificates" target="_blank" style="color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;">ABOUT SSL CERTIFICATES</a>
                </td>
            </tr>
        </table>
    </div>
</div>