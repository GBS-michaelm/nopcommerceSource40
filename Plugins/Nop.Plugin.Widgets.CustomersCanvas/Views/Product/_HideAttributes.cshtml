@model List<int>

@foreach (var id in Model)
{
    <script type="text/ecmascript">
        $(document).ready(function() {
            var dd = $('[name*="product_attribute_@id"]').parent();
            dd.css('display', 'none');
            dd.prev().css('display', 'none');
            var dt = $('[id*="product_attribute_label_@id"]');
            dt.css('display', 'none');
        });
    </script>
}